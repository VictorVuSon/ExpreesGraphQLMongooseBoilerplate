{
  "name": "express-mongoose-es6-rest-api",
  "version": "5.0.0",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "start": "nodemon server/build.js --exec babel-node",
    "start:debug": "cross-env DEBUG=express-mongoose-es6-rest-api:* yarn start",
    "pm2_start": "pm2 start server/build.js",
    "pm2_stop": "pm2 stop server/build.js",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "2.0.1",
    "bluebird": "3.5.1",
    "chai": "4.1.2",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "cross-env": "5.2.0",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "ejs": "2.6.1",
    "express": "4.16.3",
    "express-graphql": "0.6.12",
    "express-validation": "1.0.2",
    "express-validator": "5.2.0",
    "express-winston": "2.5.0",
    "graphql": "0.13.2",
    "heapdump": "0.3.9",
    "helmet": "3.12.0",
    "http-status": "1.0.1",
    "joi": "10.6.0",
    "jsonwebtoken": "8.3.0",
    "memwatch-next": "0.3.0",
    "method-override": "^2.3.10",
    "mongoose": "5.1.5",
    "morgan": "1.9.0",
    "uuid": "^3.1.0",
    "winston": "^3.0.0-rc5",
    "winston-daily-rotate-file": "3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "*",
    "babel-preset-stage-2": "^6.24.1",
    "mocha": "3.5.0",
    "nodemon": "^1.11.0",
    "supertest": "3.0.0",
    "supertest-as-promised": "4.0.2"
  },
  "license": "MIT"
}
